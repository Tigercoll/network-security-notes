{
  "name": "enhance_daily_document",
  "description": "Enhance and complete daily learning documents in D:\projects\Network Security\daily\ with detailed content based on existing patterns",
  "version": "1.0.0",
  "author": "Claude",
  "commands": [
    {
      "name": "enhance",
      "description": "Enhance a single daily document with detailed content",
      "parameters": {
        "type": "object",
        "properties": {
          "day_number": {
            "type": "string",
            "description": "Day number to enhance (e.g., '001', '050')",
            "default": "001"
          },
          "verbose": {
            "type": "boolean",
            "description": "Show detailed analysis and changes",
            "default": true
          }
        },
        "required": ["day_number"]
      }
    },
    {
      "name": "enhance_all",
      "description": "Enhance all daily documents in the daily folder",
      "parameters": {
        "type": "object",
        "properties": {
          "start_day": {
            "type": "string",
            "description": "Start day number (default: 001)",
            "default": "001"
          },
          "end_day": {
            "type": "string",
            "description": "End day number (default: 090)",
            "default": "090"
          },
          "skip_existing": {
            "type": "boolean",
            "description": "Skip documents that are already complete",
            "default": true
          }
        }
      }
    },
    {
      "name": "analyze",
      "description": "Analyze document completeness without modifying",
      "parameters": {
        "type": "object",
        "properties": {
          "day_number": {
            "type": "string",
            "description": "Day number to analyze"
          }
        },
        "required": ["day_number"]
      }
    }
  ],
  "examples": [
    "Enhance Day050 document to add detailed exercises and references",
    "Analyze Day020 for missing sections",
    "Enhance all documents from Day060 to Day070"
  ],
  "requirements": {
    "python_version": ">=3.9"
  }
}
